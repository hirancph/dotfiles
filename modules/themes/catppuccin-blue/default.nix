{
  polarity = "dark";
  
  # Backgrounds (Extracted from wallpaper)
  base00 = "141720"; # Deepest background (Wallpaper main dark)
  base01 = "1a1d28"; # Lighter background (Status bars, UI elements)
  base02 = "212532"; # Selection background
  base03 = "2e3444"; # Comments / UI Borders
  base04 = "383f52"; # Dark foreground / UI Highlights

  # Foregrounds (Catppuccin Mocha Standard)
  base05 = "cdd6f4"; # Default Foreground (Text)
  base06 = "f5e0dc"; # Light Foreground (Rosewater)
  base07 = "b4befe"; # Light Background (Lavender)

  # Accents (Catppuccin Mocha Standard)
  base08 = "f38ba8"; # Red (Variables, Errors)
  base09 = "fab387"; # Peach (Integers, Constants)
  base0A = "f9e2af"; # Yellow (Classes, Search Text)
  base0B = "a6e3a1"; # Green (Strings)
  base0C = "94e2d5"; # Teal (Regex, Escape Chars)
  base0D = "89b4fa"; # Blue (Functions, Headings)
  base0E = "cba6f7"; # Mauve (Keywords)
  base0F = "f2cdcd"; # Flamingo (Deprecated, Tags)
}
